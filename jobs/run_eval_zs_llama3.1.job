#!/bin/bash

#SBATCH --partition=gpu_h100
#SBATCH --gpus=4
#SBATCH --job-name=EVZS
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=8
#SBATCH --time=1:00:00
#SBATCH --output=outputs/eval/zs/%A.out

module purge
module load 2024
module load Anaconda3/2024.06-1

cd $HOME/nlp2/

source activate nlp2
srun python -u src/main_eval.py \
    --model_path "meta-llama/Llama-3.1-8b" \
    --num_gpus 4 \
    --system_prompt_path "config/prompts/gsm8k.txt" \
    --eval_dataset "openai/gsm8k" \
    --eval_dataset_config_name "main" \
    --batch_size 1319 \
    --max_tokens 16000 \
    --output_dir "./model_outputs/gsm8k"
conda deactivate