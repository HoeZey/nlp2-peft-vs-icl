#!/bin/bash

#SBATCH --partition=gpu_h100
#SBATCH --gpus=2
#SBATCH --job-name=EV1S
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=8
#SBATCH --time=1:00:00
#SBATCH --output=outputs/eval/%A_llama3_1s.out

module purge
module load 2024
module load Anaconda3/2024.06-1

cd $HOME/nlp2/

source activate nlp2
srun python -u src/main_eval.py \
    --model_path "meta-llama/Llama-3.1-8b" \
    --num_gpus 2 \
    --system_prompt_path "config/prompts/gsm8k.txt" \
    --eval_dataset "openai/gsm8k" \
    --eval_dataset_config_name "main" \
    --icl_dataset "openai/gsm8k" \
    --icl_dataset_config_name "main" \
    --k 1 \
    --batch_size 1319 \
    --max_tokens 8000 \
    --temperature 0.4 \
    --temperature 0.95 \
    --output_dir "./model_outputs/gsm8k"
conda deactivate